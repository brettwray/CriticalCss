{
  "name": "burtwraynolds/critical-css",
  "description": "A Laravel package for generating and using inline critical-path CSS.",
  "keywords": ["css", "performance", "laravel"],
  "authors": [
    {
      "name": "Brett Wray",
      "email": "brett@brettwray.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.0",
    "illuminate/contracts": "^7",
    "illuminate/support": "^7",
    "illuminate/console": "^7",
    "illuminate/http": "^7",
    "illuminate/view": "^7",
    "symfony/process": "^5",
    "phpoption/phpoption": "^1.7",
    "vlucas/phpdotenv": "^4|^5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "mockery/mockery": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "burtwraynolds\\CriticalCss\\": "./src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "config": {
      "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
